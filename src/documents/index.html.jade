---
layout: default
---

h1 Generator
fieldset
  legend Paramètres du mot de passe
  label Longueur minimale :
    input#nbminlen(type="number", required, min="1", max="16", pattern="^\\\\d{1,2}$")
  label Longueur maximale :
    input#nbmaxlen(type="number", required, min=3, max=16, pattern="^\\\\d{1,2}$")
  label Alphabet :
    input#alphabet(type="text", required="required")
fieldset.contraintes
  legend Contraintes
  label Au moins
    input#nbnumber.unchiffre(type="number", required, min="0", max="3", pattern="^\\\\d{0,1}$")
    |  chiffre.
  label Au moins
    input#nblower.unchiffre(type="number", required, min="0", max="3", pattern="^\\\\d{0,1}$"))
    |  lettre minuscule.
  label Au moins
    input#nbupper.unchiffre(type="number", required, min="0", max="3", pattern="^\\\\d{0,1}$"))
    |  lettre majuscule.
  label Au moins
    input#nbspecial.unchiffre(type="number", required, min="0", max="3", pattern="^\\\\d{0,1}$"))
    |  caractère spécial.
fieldset.f_input.inline
  legend Fichier en entrée
  textarea#input(wrap='off').
    DUPONT Jean
    DURAND Jean-Claude
    L'ALLAN Florence
    DE LA LOUSE Bernard
    UNAMIAMOA M'hijiba
    O'CONNAR O'Ryan
  br
  label Séparateur nom/prénom :
    select#selectseparator()
      option(value=" ") Espace
      option(value=";") Point virgule
      option(value="") Aucun
  br
  label
    span.chkboxctn
      input#checkboxfirstnamefirst(type='checkbox')
     |  Prénom en premier
fieldset.f_output.inline
  legend Sortie
  textarea#output(readonly='readonly', wrap='off')
fieldset
  legend Opérations
  p
    a.btn.btn-primary#resetparambtn Réinitialiser les paramètres
    |&nbsp;
    a.btn.btn-primary#gobtn Générer

